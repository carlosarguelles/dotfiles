{
  "console.log": {
    "prefix": "cl",
    "body": ["console.log($1)"],
    "description": "console.log"
  },
  "public async": {
    "prefix": "pubas",
    "body": ["public async $1 {", "\t$2", "}"]
  },
  "Express callback": {
    "prefix": "expc",
    "body": ["async (req: Request, res: Response) => {", "$1", "}"]
  },
  "Export default": {
    "prefix": "exdef",
    "body": ["export default $1"]
  },
  "import default": {
    "prefix": "idf",
    "body": ["import $1 from \"$2\""]
  }
}
