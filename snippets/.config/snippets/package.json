{
  "name": "snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "rmd",
          "rmarkdown"
        ],
        "path": "./snippets/rmarkdown.json"
      },
      {
        "language": [
          "rmd",
          "rmarkdown",
          "plaintex",
          "tex"
        ],
        "path": "./snippets/latexmath.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/tsreact.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/ts.json"
      },
      {
        "language": "all",
        "path": "./snippets/andrea.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/andreabackend.json"
      }
    ]
  }
}
